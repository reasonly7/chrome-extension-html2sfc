{
  "manifest_version": 3,
  "name": "Hello World Extension",
  "version": "1.0",
  "description": "Print 'Hello, World!' in the console on page load.",
  "permissions": [],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
